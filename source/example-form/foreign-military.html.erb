---
layout: "example-form-layout"
title: eApp Design System | Example form
---

<form action="" class="eapp-form" data-id="Section 4: Military History - Foreign Military">
	<h1 class="eapp-section-header">Foreign Military</h1>

	<!-- Member of foreign military -->
	<fieldset class="usa-fieldset-inputs">
		<legend>Have you EVER served as a civilian or military member in a foreign countryâ€™s military, intelligence, diplomatic, security forces, militia, other defense force, or government agency?</legend>
		<ul class="usa-list-inline usa-list-inline-justified">
			<li>
        <input class="yes-radio" id="yes-1" type="radio" name="foriegn-military" data-id="foriegn-military-yes">
        <label for="yes-1"><span>Yes</span></label>
      </li>
      <li>
        <input class="no-radio" id="no-1" type="radio" name="foriegn-military">
        <label for="no-1"><span>No</span></label>
      </li>
		</ul>
	</fieldset>
	<!-- Member of foreign military end -->
	<div class="foriegn-military-yes" style="display: none;">
		<ul id="accordion" class="usa-accordion-bordered">
	    <li>
	      <div class="usa-accordion-header">
	        <button class="usa-accordion-button"
	          aria-expanded="true"
	          aria-controls="b-a1">
	          <div class="titles">
	            <div class="title">
	              <div class="title-right">
	                <span class="fa-stack">
	                  <i class="fas fa-circle fa-stack-2x"></i>
	                  <i class="fas fa-stack-1x fa-shield-alt fa-inverse"></i>
	                </span>
	                <span>Foreign organization 1:</span>
	              </div>
	              <div class="title-left">
	              </div>
	            </div>
	          </div>
	          <div class="usa-accordion-close-button">
	          </div>
	        </button>
	        <button class="usa-accordion-remove-button">
	          <span class="fas fa-times fa-fw"></span>
	          <span class="remove">Remove</span>
	        </button>
	      </div>
	      <div id="b-a1" class="usa-accordion-content">
	      	<!-- Which foreign organization -->
					<fieldset class="usa-fieldset-inputs">
						<legend>During your foreign service, which organization were you serving under</legend>
						<ul class="usa-unstyled-list">
							<li>
								<input id="military" type="radio" name="foreign-military-branch" value="military">
				        <label for="military"><span>Military (Army, Navy, Air Force, Marines, etc.)</span></label>
							</li>
							<li>
								<input id="intelligence-service" type="radio" name="foreign-military-branch" value="intelligence-service">
				        <label for="intelligence-service"><span>Intelligence Service</span></label>
							</li>
							<li>
								<input id="diplomatic-service" type="radio" name="foreign-military-branch" value="diplomatic-service">
				        <label for="diplomatic-service"><span>Diplomatic Service</span></label>
							</li>
							<li>
								<input id="security-forces" type="radio" name="foreign-military-branch" value="security-forces">
				        <label for="security-forces"><span>Security Forces</span></label>
							</li>
							<li>
								<input id="militia" type="radio" name="foreign-military-branch" value="militia">
				        <label for="militia"><span>Militia</span></label>
							</li>
							<li>
								<input id="other-defense-forces" type="radio" name="foreign-military-branch" value="other-defense-forces">
				        <label for="other-defense-forces"><span>Other Defense Forces</span></label>
							</li>
							<li>
								<input id="other-gov-agency" type="radio" name="foreign-military-branch" value="other-gov-agency">
				        <label for="other-gov-agency"><span>Other Government Agency</span></label>
							</li>
						</ul>
					</fieldset>
					<!-- Which foreign organization end -->
					<!-- Name of foreign organization -->
					<div class="usa-form-control">
						<label for="name-foreign-org" class="usa-alt-label">Provide the name of the foreign organization</label>
						<input type="text" id="name-foreign-org">
					</div>
					<!-- Name of foreign organization end -->
					<!-- Period of serivice -->
					<fieldset id="from-to-date-block" class="usa-fieldset-inputs">
			      <legend>
			      	<span>Provide your period of service</span>
			      	<a class="usa-help-icon usa-help-trigger" href="#" onclick="return false;">
			          <i class="fas fa-question-circle"></i>
			        </a>
			      </legend>
			      <div class="from-to-dates">
			        <div class="from-date-container">
			          <p><strong>From date</strong></p>
			          <div class="m-d-y-block">
			            <div class="usa-form-control">
			              <label for="month">Month</label>
			              <input type="text" id="month" maxlength="2">
			            </div>
			            <div class="usa-form-control">
			              <label for="day">Day</label>
			              <input type="text" id="day" maxlength="2">
			            </div>
			            <div class="usa-form-control">
			              <label for="year">Year</label>
			              <input type="text" id="year" maxlength="4">
			            </div>
			          </div>
			          <div class="modifiers modifiers-left">
			            <div class="modifier">
			              <input id="from-estimated" type="checkbox" name="from-estimated">
			              <label class="simple-checkbox" for="from-estimated"><span>Estimated</span></label>
			            </div>
			          </div>
			        </div>
			        <div class="from-to-date-arrow">
			          <i class="fas fa-long-arrow-alt-right fa-2x"></i>
			        </div>
			        <div class="to-date-container">
			          <p><strong>To date</strong></p>
			          <div class="m-d-y-block">
			            <div class="usa-form-control">
			              <label for="month">Month</label>
			              <input type="text" id="month" class="to-date-input-1" maxlength="2">
			            </div>
			            <div class="usa-form-control">
			              <label for="day">Day</label>
			              <input type="text" id="day" class="to-date-input-1" maxlength="2">
			            </div>
			            <div class="usa-form-control">
			              <label for="year">Year</label>
			              <input type="text" id="year" class="to-date-input-1" maxlength="4">
			            </div>
			          </div>
			          <div class="modifiers modifiers-left">
			            <div class="modifier">
			              <input id="to-estimated" class="to-date-input-1" type="checkbox" name="to-estimated">
			              <label class="simple-checkbox" for="to-estimated"><span>Estimated</span></label>
			            </div>
			          </div>
			        </div>
			        <div class="or">or</div>
			        <div class="present">
			          <input class="disabled-toggle" id="present" type="checkbox" name="present" data-id="to-date-input-1">
			          <label for="present"><span>Present</span></label>
			        </div>
			      </div>
			    </fieldset>
			    <!-- Period of service end -->
					<!-- Name of country -->
					<div class="usa-form-control">
						<label for="name-foreign-org-country" class="usa-alt-label">Provide the name of the country</label>
						<input type="text" id="country-select">
					</div>
					<!-- Name of country end -->
					<!-- Division/department -->
					<div class="usa-form-control">
						<label for="foreign-division-department" class="usa-alt-label">Provide the division/department/office in which you served</label>
						<input type="text" id="foreign-division-department">
					</div>
					<!-- Division/department end -->
					<!-- Description of circumstances -->
					<div class="usa-form-control">
						<label for="description-circumstances" class="usa-alt-label">Provide a description of the circumstances of your association with this organization</label>
						<textarea name="" id="description-circumstances" cols="30" rows="10"></textarea>
					</div>
					<!-- Description of circumstances end -->
					<!-- Reason for leaving -->
					<div class="usa-form-control">
						<label for="reason-leaving" class="usa-alt-label">Provide a description of the reason for leaving this service</label>
						<textarea name="" id="reason-leaving" cols="30" rows="10"></textarea>
					</div>
					<!-- Reason for leaving end -->
	      </div>
	    </li>
	  </ul>
	  <!-- Additional foreign military -->
	  <fieldset class="usa-fieldset-inputs">
			<legend>Do you have an additional foreign military service to report?</legend>
			<ul class="usa-list-inline usa-list-inline-justified">
				<li>
	        <input id="yes-2" type="radio" name="additional-foreign-military">
	        <label for="yes-2"><span>Yes</span></label>
	      </li>
	      <li>
	        <input id="no-2" type="radio" name="additional-foreign-military">
	        <label for="no-2"><span>No</span></label>
	      </li>
			</ul>
		</fieldset>
	</div>
  <!-- Back/next -->
	<div class="next-back-block">
  	<% link_to '/example-form/diciplinary-procedures.html', :class => "usa-button back-button" do %>
      <i class="far fa-arrow-alt-circle-left"></i>
      <div class="direction">Back</div>
      <div class="label">Diciplinary procedures</div>
    <% end %>
    <% link_to '/example-form/us-passport.html', :class => "usa-button next-button" do %>
      <i class="far fa-arrow-alt-circle-right"></i>
      <div class="direction">Next</div>
      <div class="label">U.S. passport information</div>
    <% end %>
  </div>
  <!-- Back/next end -->
</form>