---
layout: "example-form-layout"
title: eApp Design System | Example form
---

<form action="" class="eapp-form" data-id="Section 4: Military History - U.S Military">
	<h1 class="eapp-section-header">U.S. Military</h1>

	<!-- Ever served in U.S. military -->
	<fieldset class="usa-fieldset-inputs">
		<legend>Have you EVER served in the U.S. Military?</legend>
		<ul class="usa-list-inline usa-list-inline-justified">
			<li>
        <input class="yes-radio" id="yes-1" type="radio" name="us-military" data-id="us-military-yes">
        <label for="yes-1"><span>Yes</span></label>
      </li>
      <li>
        <input class="no-radio" id="no-1" type="radio" name="us-military">
        <label for="no-1"><span>No</span></label>
      </li>
		</ul>
	</fieldset>
	<!-- Ever served in U.S. military end -->
	<!-- Military branch -->
	<div class="us-military-yes" style="display: none;">
		<ul id="accordion" class="usa-accordion-bordered">
	    <li>
	      <div class="usa-accordion-header">
	        <button class="usa-accordion-button"
	          aria-expanded="true"
	          aria-controls="b-a1">
	          <div class="titles">
	            <div class="title">
	              <div class="title-right">
	                <span class="fa-stack">
	                  <i class="fas fa-circle fa-stack-2x"></i>
	                  <i class="fas fa-stack-1x fa-shield-alt fa-inverse"></i>
	                </span>
	                <span>Branch 1:
	              </div>
	              <div class="title-left">
	              </div>
	            </div>
	          </div>
	          <div class="usa-accordion-close-button">
	          </div>
	        </button>
	        <button class="usa-accordion-remove-button">
	          <span class="fas fa-times fa-fw"></span>
	          <span class="remove">Remove</span>
	        </button>
	       </div>
	      <div id="b-a1" class="usa-accordion-content">
					<fieldset class="usa-fieldset-inputs">
						<legend>Provide the branch of service you served in:</legend>
						<ul class="usa-unstyled-list">
							<li>
								<input id="army" type="radio" name="military-branch" value="army">
				        <label for="army"><span>Army</span></label>
							</li>
							<li>
								<input id="army-national-guard" type="radio" name="military-branch" value="army-national-guard">
				        <label for="army-national-guard"><span>Army National Guard</span></label>
							</li>
							<li>
								<input id="navy" type="radio" name="military-branch" value="navy">
				        <label for="navy"><span>Navy</span></label>
							</li>
							<li>
								<input id="air-force" type="radio" name="military-branch" value="air-force">
				        <label for="air-force"><span>Air Force</span></label>
							</li>
							<li>
								<input id="air-nationa-guard" type="radio" name="military-branch" value="air-nationa-guard">
				        <label for="air-nationa-guard"><span>Air National Guard</span></label>
							</li>
							<li>
								<input id="marine-corps" type="radio" name="military-branch" value="marine-corps">
				        <label for="marine-corps"><span>Marine Corps</span></label>
							</li>
							<li>
								<input id="coast-guard" type="radio" name="military-branch" value="coast-guard">
				        <label for="coast-guard"><span>Coast Guard</span></label>
							</li>
						</ul>
					</fieldset>
					<!-- Military branch end -->
					<!-- Military status -->
					<fieldset class="usa-fieldset-inputs">
						<legend>Provide your status</legend>
						<ul class="usa-list-inline">
							<li>
								<input id="active-duty" type="radio" name="military-status" value="active-duty">
				        <label for="active-duty"><span>Active Duty</span></label>
							</li>
							<li>
								<input id="active-reserve" type="radio" name="military-status" value="active-reserve">
				        <label for="active-reserve"><span>Active Reserve</span></label>
							</li>
							<li>
								<input id="inactive-reserve" type="radio" name="military-status" value="inactive-reserve">
				        <label for="inactive-reserve"><span>Inactive Reserve</span></label>
							</li>
						</ul>
					</fieldset>
					<!-- Military status end -->
					<!-- Officer or enlisted -->
					<fieldset class="usa-fieldset-inputs">
						<legend>Officer or enlisted</legend>
						<ul class="usa-list-inline">
							<li>
								<input id="officer" type="radio" name="officer-enlisted" value="officer">
				        <label for="officer"><span>Officer</span></label>
							</li>
							<li>
								<input id="enlisted" type="radio" name="officer-enlisted" value="enlisted">
				        <label for="enlisted"><span>Enlisted</span></label>
							</li>
							<li>
								<input id="not-applicable-1" type="radio" name="officer-enlisted" value="not-applicable-1">
				        <label for="not-applicable-1"><span>Not applicable</span></label>
							</li>
						</ul>
					</fieldset>
					<!-- Officer or enlisted end -->
					<!-- Service number -->
					<div class="usa-form-control">
						<label for="service-number" class="usa-alt-label">Provide your service number</label>
						<input type="text" id="service-number">
					</div>
					<!-- Service number end -->
					<!-- Dates of service -->
					<fieldset id="from-to-date-block" class="usa-fieldset-inputs">
			      <legend>
			      	<span>Provide your dates of service</span>
			      	<a class="usa-help-icon usa-help-trigger" href="#" onclick="return false;">
			          <i class="fas fa-question-circle"></i>
			        </a>
			      </legend>
			      <p>You are not required to list temporary locations of less than 90 days that did not serve as your permanent or mailing address.</p>
			      <div class="from-to-dates">
			        <div class="from-date-container">
			          <p><strong>From date</strong></p>
			          <div class="m-d-y-block">
			            <div class="usa-form-control">
			              <label for="month">Month</label>
			              <input type="text" id="month" maxlength="2">
			            </div>
			            <div class="usa-form-control">
			              <label for="day">Day</label>
			              <input type="text" id="day" maxlength="2">
			            </div>
			            <div class="usa-form-control">
			              <label for="year">Year</label>
			              <input type="text" id="year" maxlength="4">
			            </div>
			          </div>
			          <div class="modifiers modifiers-left">
			            <div class="modifier">
			              <input id="from-estimated" type="checkbox" name="from-estimated">
			              <label class="simple-checkbox" for="from-estimated"><span>Estimated</span></label>
			            </div>
			          </div>
			        </div>
			        <div class="from-to-date-arrow">
			          <i class="fas fa-long-arrow-alt-right fa-2x"></i>
			        </div>
			        <div class="to-date-container">
			          <p><strong>To date</strong></p>
			          <div class="m-d-y-block">
			            <div class="usa-form-control">
			              <label for="month">Month</label>
			              <input type="text" id="month" class="to-date-input-1" maxlength="2">
			            </div>
			            <div class="usa-form-control">
			              <label for="day">Day</label>
			              <input type="text" id="day" class="to-date-input-1" maxlength="2">
			            </div>
			            <div class="usa-form-control">
			              <label for="year">Year</label>
			              <input type="text" id="year" class="to-date-input-1" maxlength="4">
			            </div>
			          </div>
			          <div class="modifiers modifiers-left">
			            <div class="modifier">
			              <input id="to-estimated" class="to-date-input-1" type="checkbox" name="to-estimated">
			              <label class="simple-checkbox" for="to-estimated"><span>Estimated</span></label>
			            </div>
			          </div>
			        </div>
			        <div class="or">or</div>
			        <div class="present">
			          <input class="disabled-toggle" id="present" type="checkbox" name="present" data-id="to-date-input-1">
			          <label for="present"><span>Present</span></label>
			        </div>
			      </div>
			    </fieldset>
			    <!-- Dates of service end -->
			    <!-- Discharged -->
			    <fieldset class="usa-fieldset-inputs">
						<legend>Where you discharged from this instance of U.S. military service, to include Reserves, or National Guard?</legend>
						<ul class="usa-list-inline usa-list-inline-justified">
							<li>
				        <input class="yes-radio" id="yes-2" type="radio" name="us-military" data-id="discharged-yes">
				        <label for="yes-2"><span>Yes</span></label>
				      </li>
				      <li>
				        <input class="no-radio" id="no-2" type="radio" name="us-military">
				        <label for="no-2"><span>No</span></label>
				      </li>
						</ul>
					</fieldset>
					<!-- Discharged end -->
					<div class="discharged-yes" style="display: none;">
						<!-- Type of discharge -->
						<fieldset class="usa-fieldset-inputs">
							<legend>Provide the type of discharge you recieved</legend>
							<ul class="usa-unstyled-list">
								<li>
									<input class="no-radio" id="honorable" type="radio" name="discharge-type" value="honorable">
					        <label for="honorable"><span>Honorable</span></label>
								</li>
								<li>
									<input class="yes-radio" id="dishonorable" type="radio" name="discharge-type" value="dishonorable" data-id="discharge-reason">
					        <label for="dishonorable"><span>Dishonorable</span></label>
								</li>
								<li>
									<input class="yes-radio" id="under-other-honorable" type="radio" name="discharge-type" value="under-other-honorable" data-id="discharge-reason">
					        <label for="under-other-honorable"><span>Under other honorable conditions</span></label>
								</li>
								<li>
									<input class="yes-radio" id="general" type="radio" name="discharge-type" value="general" data-id="discharge-reason">
					        <label for="general"><span>General</span></label>
								</li>
								<li>
									<input class="yes-radio" id="bad-conduct" type="radio" name="discharge-type" value="bad-conduct" data-id="discharge-reason">
					        <label for="bad-conduct"><span>Bad conduct</span></label>
								</li>
								<li>
									<input id="other-2" type="radio" name="discharge-type" value="other-2">
					        <label for="other-2"><span>Other</span></label>
								</li>
							</ul>
						</fieldset>
						<!-- Type of discharge end -->
						<div class="discharge-reason" style="display: none;">
							<div class="usa-form-control">
								<label for="discharge-reason" class="usa-alt-label">Provide the reason(s) for the discharge</label>
								<textarea name="discharge-reason" id="discharge-reason" cols="30" rows="10"></textarea>
							</div>
						</div>
						<!-- Discharge date -->
						<fieldset class="usa-fieldset-inputs">
				      <legend>Provide the date of discharge listed above</legend>
				      <div class="m-d-y-block">
				        <div class="usa-form-control">
				          <label for="month">Month</label>
				          <input type="text" id="month" maxlength="2">
				        </div>
				        <div class="usa-form-control">
				          <label for="day">Day</label>
				          <input type="text" id="day" maxlength="2">
				        </div>
				        <div class="usa-form-control">
				          <label for="year">Year</label>
				          <input type="text" id="year" maxlength="4">
				        </div>
				      </div>
				      <div class="modifiers modifiers-left">
				        <div class="modifier">
				          <input id="estimated" type="checkbox" name="estimated">
				          <label class="simple-checkbox" for="estimated"><span>Estimated</span></label>
				        </div>
				      </div>
				    </fieldset>
					</div>
	      </div>
	    </li>
	  </ul>
	  <!-- Additional U.S. military -->
	  <fieldset class="usa-fieldset-inputs">
			<legend>Do you have additional military service to report?</legend>
			<ul class="usa-list-inline usa-list-inline-justified">
				<li>
	        <input id="yes-3" type="radio" name="additional-us-military">
	        <label for="yes-3"><span>Yes</span></label>
	      </li>
	      <li>
	        <input id="no-3" type="radio" name="additional-us-military">
	        <label for="no-3"><span>No</span></label>
	      </li>
			</ul>
		</fieldset>
		<!-- Additional military end -->
	</div>
	<!-- Back/next -->
	<div class="next-back-block">
  	<% link_to '/example-form/selective-service.html', :class => "usa-button back-button" do %>
      <i class="far fa-arrow-alt-circle-left"></i>
      <div class="direction">Back</div>
      <div class="label">Selective service status</div>
    <% end %>
    <% link_to '/example-form/diciplinary-procedures.html', :class => "usa-button next-button" do %>
      <i class="far fa-arrow-alt-circle-right"></i>
      <div class="direction">Next</div>
      <div class="label">Diciplinary procedures</div>
    <% end %>
  </div>
  <!-- Back/next end -->
</form>